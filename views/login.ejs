<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <script src="https://kit.fontawesome.com/1f7012dcb5.js" crossorigin="anonymous" defer></script>
    <!-- external script -->
    <script src="/js/message.js" defer></script>
    <!--  bootstrap css -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link type="text/css" rel="stylesheet" href="/css/style.css">

    <title>Login</title>
</head>
<body>
    
    
<!-- messages -->
<div style="height:0">
   <div class="row">
      <div class="message">
           <%- messages('message', locals) %>
      </div>
   </div>
</div>


<!-- /.container -->
<main role="main" class="container">
  <div class="row">
     <div class="col-xl-4 col-lg-6 col-md-8 col-sm-10 mx-auto">
     
     
       <!-- main naviagtion -->
        <div class="main-nav login">
            <nav>
                <ul>
                    <li><a class="btn btn-primary" href="/">Home</a></li>
                    <li><a class="btn btn-primary" href="/auth/login">Login</a></li>
                    <li><a class="btn btn-primary" href="/profile">Profile</a></li>
                </ul>
            </nav>
        </div>

       
        <!-- main title -->
        <div class="main-title login">
            <header>
                <h3>Login using...</h3>
            </header>
        </div>

       
        <!-- main content -->
        <div class="main-content login">
            <div>
                <a class="google-btn btn btn-sm btn-danger" href="/auth/google">Google</a><br><br>
            </div>
        </div>


       <!-- main footer -->
        <div class="main-footer home ">
            <div>&copy; dev010_1 2021</div>
        </div>

      </div>
   </div>  
</main>
<!-- /.container -->


<!-- bootstrap javascript -->
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
</body>
</html>